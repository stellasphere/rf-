{
  "manifest_version": 2,
  "name": "RF++",
  "version": "1.0.0",
  "permissions": ["webNavigation","*://*.roboflow.com/*"],
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "extras/*"
  ]
}
